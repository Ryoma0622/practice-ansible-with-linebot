<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.practice.linebot.UserRepository">
  <select id="findByUserName" resultType="com.practice.linebot.User">
    select *
    from login_user
    where user_name = #{userName}
  </select>
</mapper>